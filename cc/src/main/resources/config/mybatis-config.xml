<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 基础设置 -->
	<settings>
		<!-- changes from the defaults，全局启用或禁用延迟加载。当禁用时，所有关联对象都会即时加载。 -->
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>
		 
	<!-- 别名定义 -->
	<typeAliases>
		<typeAlias alias="String" type="java.lang.String" /> 
	</typeAliases>
		

	<!-- SQL映射文件 -->
	<mappers>
		<mapper resource="mapper/SystemParamsMapper.xml" />
		<mapper resource="mapper/JobCenterMapper.xml" />
		<mapper resource="mapper/JobExecuteLogMapper.xml" />
		<mapper resource="mapper/SystemInfoMapper.xml" />
		<mapper resource="mapper/HolidayMapper.xml" />
	</mappers>
</configuration>
