	<sql id="findPage_condition">
		<where>
			<if test="param!=null">
				<!-- 查询条件根据业务需求自由添加 -->
				<if test="param.code != null and param.code !='' ">
					<![CDATA[and code =#{param.code}]]>
				</if>
				<if test="param.productCode != null and param.productCode !='' ">
					<![CDATA[and product_code =#{param.productCode}]]>
				</if>
				<if test="param.delFlag != null and param.delFlag !='' ">
					<![CDATA[and del_flag =#{param.delFlag}]]>
				</if>
			</if>
			<if test="inCodes != null and inCodes.size > 0">
				and code IN 
				<foreach item="item" index="index" collection="inCodes" open="(" separator="," close=")">  
				  	#{item}  
				 </foreach>  
			</if>
		</where>
	</sql>
	<!-- 不分页查询 -->
	<select id="findProductSkus" resultMap="FindResultMap"
		parameterType="com.lj.eshop.dto.FindProductSkuPage">
		SELECT
		<include refid="Base_Column_List" />
		FROM t_product_sku
		<include refid="findPage_condition" />
		order by order_no
	</select>
	<!-- 分页查询 -->
	<select id="findProductSkuPage" resultMap="FindResultMap"
		parameterType="com.lj.eshop.dto.FindProductSkuPage">
		SELECT
		<include refid="Base_Column_List" />
		FROM t_product_sku
		<include refid="findPage_condition" />
		order by order_no
		limit ${start} , ${limit}
	</select>
	<!-- 查询总数 -->
	<select id="findProductSkuPageCount" resultType="int"
		parameterType="com.lj.eshop.dto.FindProductSkuPage">
		select count(*)
		FROM t_product_sku
		<include refid="findPage_condition" />
	</select>

